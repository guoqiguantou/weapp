    ygzsrtksum: null,
    pjjtksum: null,
    sczpcsum: null,
    yeardata: [new Date().getFullYear(), new Date().getFullYear() - 1],
    this_year_zxl: [],
    this_year_zsr: [],
    this_year_pjj: [],
    last_year_zxl: [],
    last_year_zsr: [],
    last_year_pjj: [],
    
    this_year_zxl_sum:null,
    this_year_zsr_sum: null,
    this_year_pjj_sum: null,
    last_year_zxl_sum: null,
    last_year_zsr_sum: null,
    last_year_pjj_sum: null,
  },
  //获取报表列表
  getAlllist() {
    $request({
      url: app.globalData.serverBook + "sell/selectSellStatement",
      method: "POST",
      type: 'form',
    }).then(res => {
      if (res.data.resultCode == 0) {
        let dataname = [];
        let this_year_zxl = [];
        let this_year_zsr = [];
        let this_year_pjj = [];
        let last_year_zxl = [];
        let last_year_zsr = [];
        let last_year_pjj = [];
        let datalist = res.data.data.data;
        let datasum = res.data.data.sum;
        for (let i in datalist) {
          dataname.push(datalist[i].name)
          this_year_zxl.push(datalist[i].data['this_year'].allOutPut)
          this_year_zsr.push(datalist[i].data['this_year'].allIn)
          this_year_pjj.push(datalist[i].data['this_year'].average_money)
          last_year_zxl.push(datalist[i].data['last_year'].allOutPut)
          last_year_zsr.push(datalist[i].data['last_year'].allIn)
          last_year_pjj.push(datalist[i].data['last_year'].average_money)
        }


        this.setData({
          dataname: dataname,
          this_year_zxl: this_year_zxl,
          this_year_zsr: this_year_zsr,
          this_year_pjj: this_year_pjj,
          last_year_zxl: last_year_zxl,
          last_year_zsr: last_year_zsr,
          last_year_pjj: last_year_pjj,
          this_year_zxl_sum: datasum.this_year.year_allOutPut,
          this_year_zsr_sum: datasum.this_year.year_allIn,
          this_year_pjj_sum: datasum.this_year.year_average,
          last_year_zxl_sum: datasum.last_year.year_allOutPut,
          last_year_zsr_sum: datasum.last_year.year_allIn,
          last_year_pjj_sum: datasum.last_year.year_average,
        })
        // dataname: dataname,
        //   zxl2018: zxl2018,
        //     ygzsr2018: ygzsr2018,
        //       pjj2018: pjj2018,
        //         zxl2019: zxl2019,
        //           ygzsr2019: ygzsr2019,
        //             pjj2019: pjj2019,

        //               zxl2018sum: datasum[2018].year_allOutPut,
        //                 ygzsr2018sum: datasum[2018].year_allIn,
        //                   pjj2018sum: datasum[2018].year_average,
        //                     zxl2019sum: datasum[2019].year_allOutPut,
        //                       ygzsr2019sum: datasum[2019].year_allIn,
        //                         pjj2019sum: datasum[2019].year_average,
        //                           yeardata: Object.keys(datasum)

      } else {
        $Toast({
          content: res.data.resultDesc,
          type: 'error'
        });
      }
    }).catch(error => {


    })
  },
  //获取塘口报表
  getgroup() {
    $request({
      url: app.globalData.serverBook + "sell/selectSellStatementByGroup",
      method: "POST",
      type: 'form',
    }).then(res => {
      if (res.data.resultCode == 0) {
        let zxltk = [];
        let ygzsrtk = [];
        let pjjtk = [];
        let sczpc = [];
        let groupname = [];
        let datalist = res.data.data.data;
        let datasum = res.data.data.sum;
        for (let i in datalist) {
          groupname.push(datalist[i].name);
          zxltk.push(datalist[i].data.allOutPut);
          ygzsrtk.push(datalist[i].data.allIn);
          pjjtk.push(datalist[i].data.average_money);
          sczpc.push(datalist[i].data.count);
        }
        console.log(sczpc)
        console.log(datalist, datasum)
        this.setData({
          groupname: groupname,
          zxltk: zxltk,
          ygzsrtk: ygzsrtk,
          pjjtk: pjjtk,
          sczpc: sczpc,
          zxltksum: datasum.year_allOutPut,
          ygzsrtksum: datasum.year_allIn,
          pjjtksum: datasum.year_average,
          sczpcsum: datasum.year_count,
        })

      } else {
        $Toast({
          content: res.data.resultDesc,
          type: 'error'
        });
      }
    }).catch(error => { })
  },
  /**
   * 生命周期函数--监听页面加载
