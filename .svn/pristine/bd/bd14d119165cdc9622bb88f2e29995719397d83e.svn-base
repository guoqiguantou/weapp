/* pages/accountBookList/accountBookList.wxss */
.container {
  background: #efeff4;
  min-height: 100vh;
  box-sizing: border-box;
  overflow: hidden;
}
.warp{
  padding:20rpx 30rpx;
  background: #fff;
  margin-bottom:20rpx;
}
.header{
	font-size: 32rpx;
	font-weight: bold;
	color: #333333;
}
.warpbody{
  /* display: flex;
  flex-wrap: wrap;
  justify-content: space-between; */
  padding-top:40rpx;
}
.item{
  width:25%;
  height:140rpx;
  text-align: center;
  margin-bottom:40rpx;
  float:left;
}
.warpbody:after{
  width: 0px;
	height: 0px;
	content: "";
	display: block;
	visibility: hidden;
	clear: both;
}
.itempic{
  width: 90rpx;
	height: 90rpx;
  margin:0px auto;
  border-radius: 90rpx;
	border: solid 1px #7d7d7d;
}
.itemtext{
  line-height:30rpx;
  font-size: 24rpx;
	color: #666666;
  margin-top:20rpx;
}