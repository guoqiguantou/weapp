    <image src='../../../images/topbj3.jpg'></image>
    <view class='bannertext'>
      <view class='bannertextt'>预估总收入（元）</view>
      <view class='bannertextb'>￥1,450,000.00</view>
    </view>
  </view>

  <view class='warp'>
    <view class='header'>预估收入（4）</view>
