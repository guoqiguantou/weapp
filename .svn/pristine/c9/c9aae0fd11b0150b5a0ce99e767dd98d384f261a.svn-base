.container {
  background: #efeff4;
  min-height: 100vh;
  box-sizing: border-box;
  overflow: hidden;
}

.search_box {
  padding: 0;
}

.search_T {
  background: white !important;
}

.title {
  font-size: 30rpx;
  color: rgb(111, 111, 111);
  text-indent: 15px;
  line-height: 70rpx;
  font-weight: bold;
}

.search_T  .van-cell {
  background: #dddee1 !important;
  border-radius: 40rpx;
}
.panel_T{
  width: 96%;
  margin:0 auto !important
}
.header_box {
  padding: 0 30rpx;
  width: 90%;
}

.header_content {
  border-bottom: 1px solid #e9eaec;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.header_content_name {
  font-size: 32rpx;
  line-height: 92rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.header_content_name_img {
  width: 45rpx;
  height: 45rpx;
  box-sizing: border-box;
  border-radius: 50%;
  border: 1px solid #80848f;
  margin-right: 20rpx
}
header_content_num{
  font-weight: bold;
  padding-right: 30rpx
}
.header_content_name_icon{
  padding-left: 20rpx
}
.tag_T{
  font-size: 25rpx !important;
  line-height: 40rpx !important
}
.content_box .i-grid{
  border: 0 !important
}
.content_box_item{
  padding: 20px 2% !important;
  border: 0 !important
}
.content_box_title{
  width: auto !important;
  height: inherit !important;
  font-size: 26rpx;
  color: rgb(193,193,193);
}
.content_box_label{
  font-weight: bold;
  color: rgb(134,134,134);
  text-align: left !important;
  font-size: 35rpx !important
}
.content_box_handle{
  text-align: center;
  margin-top: 10rpx
}
.content_box_handle_btn{
  height: 30px !important;
  line-height: 30px  !important;
  border:1px solid rgb(89,163,255) !important;
  color: rgb(89,163,255) !important;
}

.move {
  position: fixed;
  width: calc(100% - 40rpx);
  right: 20rpx;
  bottom: 150rpx;
}

.addbtn {
  width: 90rpx;
  height: 90rpx;
  background-color: #3a93ff;
  box-shadow: 0px 1rpx 18rpx 0rpx rgba(3, 84, 181, 0.43);
  border-radius: 90rpx;
  text-align: center;
  line-height: 90rpx;
  margin-left: calc(100% - 90rpx);
}

.addbtn i {
  width: 55rpx;
  height: 55rpx;
  font-size: 55rpx;
  color: #fff;
}
