<!--accountBook/pages/saleslist/saleslist.wxml-->
<view class='container'>
  <view class='warp'>
    <view class='header'>费用报表</view>
    <view class='table'>
      <view class='table-left'>
        <view>名称</view>
        <view>合计</view>
        <view wx:for="{{tablenamea}}" wx:key="{{index}}">{{item}}</view>
      </view>
      <view class='table-right'>
        <view class='table-tk'>
          <view class='table-con'>
            <view class='table-tl'>
              <view>2019年</view>
              <view>￥{{tablesum2019a}}</view>
              <view wx:for="{{tabledata2019a}}" wx:key="{{index}}">￥{{item}}</view>
            </view>
            <view class='table-tl'>
              <view>2018年</view>
              <view>￥{{tablesum2018a}}</view>
              <view wx:for="{{tabledata2018a}}" wx:key="{{index}}">￥{{item}}</view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
  <view class='warp'>
    <view class='header'>临时工用工报表</view>
    <view class='table'>
      <view class='table-left'>
        <view>名称</view>
        <view wx:for="{{tablenameb}}" wx:key="{{index}}">{{item}}</view>
      </view>
      <view class='table-right'>
        <view class='table-tk'>
          <view class='table-con'>
            <view class='table-tl'>
              <view>2019年</view>
              <view wx:for="{{tabledata2019b}}" wx:key="{{index}}">￥{{item}}</view>
            </view>
            <view class='table-tl'>
              <view>2018年</view>
              <view wx:for="{{tabledata2018b}}" wx:key="{{index}}">￥{{item}}</view>
            </view>
          </view>
        </view>
      </view>
    </view>
  </view>
</view>
<i-message id="message" />
<i-toast id="toast" />
<van-dialog id="van-dialog" />